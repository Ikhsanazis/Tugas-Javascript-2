//buat program search name yang menerapkan callback function
let names = [
  "abigail",
  "alexandra",
  "alison",
  "amanda",
  "angela",
  "bella",
  "carol",
  "caroline",
  "carolyn",
  "derfe",
  "diana",
  "elizabeth",
];

function callback(array, kode) {
  var array = array.map(String);
  var array = array.map((element) => {
    return element.toLowerCase();
  });
  let newname = [];
  for (i = 0; i < array.length; i++) {
    if (array[i].includes(kode)) {
      newname.push(array[i]);
    }
  }
  console.log(newname);
  if (newname.length == 0) {
    console.log(`tidak ditemukan`);
  }
  return newname;
}

function searchName(kode, array, n, callback) {
  const newarray = callback(array, kode);

  if (newarray.length > 0 && newarray.length < n) {
    console.log(`hasil yang ditemukan kurang dari ${n}`);
  } else {
    let final = [];

    for (i = 0; i < n; i++) {
      final.push(newarray[i]);
    }
    console.log(final);
  }
}

searchName("an", nama, 3, callback);
